import Foundation
import ObjectMapper

/// This is an example model. This model is going to be deleted, probably.
/// However, Model is responsible for creating models for the fetch actions.
class MyModel: BaseObject {
  public var aaa: Int?
  public var bbb = String()
  public var ccc = "Hello Wonderer"

  override func mapping(map: Map) {
      aaa <- map["aaa"]
      bbb <- map["bbb"]
      ccc <- map["ccc"]
  }

  override func toJSON() -> [String: Any] {
    var json = [String: Any]()
    json["aaa"] = aaa
    json["bbb"] = bbb
    json["aaa"] = aaa
    return json
  }
}
